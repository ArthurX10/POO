public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        // valor de venda, percetual de desconto 
        int venda, desconto; 
        System.out.println("qual o valor da venda:");
        venda = scanner.nextInt();

        System.out.println("infome o valor do desconto: ");
        desconto = scanner.nextInt();

        double r = venda * (desconto* 0.01);
        double valorfinal = venda - r;
        double arredondado = Math.floor(valorfinal);
        System.out.println(arredondado);
        
    }